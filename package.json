{
  "name": "@hb42/sbsdb",
  "displayname": "SBS-Datenbank",
  "description": "IT-Bestand und Hotline-Tool",
  "version": "5.0.0-alpha.1170",
  "copyright": "Copyright (c) 1997-2021 Hartmut Bauer",
  "author": "hb",
  "license": "MIT",
  "main": "start-electron.js",
  "scripts": {
    "gittag": "git log -n 1 --pretty=format:\"$(git describe --tags --long) (%cd)\" --date=format:'%d.%m.%Y %H:%M:%S' > ./dist/sbsdb/resource/git.txt",
    "devel:osx": "ng build --configuration=devx",
    "devel:win": "ng build --configuration=dev",
    "devel": "npm run devel:$npm_package_config_platform",
    "prod:osx": "ng build --configuration=prodx",
    "prod:win": "ng build --configuration=prod",
    "prod": "npm run prod:$npm_package_config_platform",
    "electron:prep": "cd dist && npm install electron-window-state",
    "electron:pack:osx": "electron-packager dist sbsdb --arch x64 --icon div/electron-packager/sbasdb --overwrite --platform darwin --out ../electron-packager/ --no-prune",
    "electron:pack:win": "electron-packager dist sbsdb --arch x64 --icon div/electron-packager/sbsdb --overwrite --platform win32 --out ../electron-packager/ --no-prune",
    "electron:build": "npm run electron:prep && npm run electron:pack:$npm_package_config_platform",
    "electron:osx": "open ../electron-packager/sbsdb-darwin-x64/sbsdb.app --args dev",
    "electron:win": "../electron-packager/sbsdb-win32-x64/sbsdb.exe dev",
    "build:nv": "npm run prod",
    "dev": "npm --no-git-tag-version version prerelease && npm run devel",
    "build": "npm --no-git-tag-version version prerelease && npm run build:nv",
    "electron": "npm run electron:build && npm run electron:$npm_package_config_platform",
    "start": "npm run build && npm run electron",
    "release:pre": "npm version prerelease && npm run build:nv",
    "release:patch": "npm version patch && npm run build:nv",
    "release:minor": "npm version minor && npm run build:nv",
    "release:major": "npm version major && npm run build:nv",
    "release": "npm run release:patch"
  },
  "repository": "hb42/sbsdb",
  "config": {
    "platform": "win"
  },
  "dependencies": {
    "@angular/animations": "11.2.0",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "11.2.0",
    "@angular/core": "11.2.0",
    "@angular/forms": "11.2.0",
    "@angular/localize": "^11.2.0",
    "@angular/material": "^11.2.0",
    "@angular/platform-browser": "11.2.0",
    "@angular/platform-browser-dynamic": "11.2.0",
    "@angular/router": "11.2.0",
    "@hb42/lib-client": "^1.1.21",
    "@hb42/lib-common": "^1.0.5",
    "classlist.js": "^1.1.20150312",
    "js-base64": "^3.6.0",
    "material-design-icons-iconfont": "^6.0.0",
    "popper.js": "^1.16.1",
    "rxjs": "^6.6.3",
    "semver": "^7.3.4",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1102.0",
    "@angular-devkit/build-angular": "^0.1102.0",
    "@angular-devkit/core": "^11.2.0",
    "@angular-devkit/schematics": "^11.2.0",
    "@angular-eslint/builder": "^1.2.0",
    "@angular-eslint/eslint-plugin": "^1.2.0",
    "@angular-eslint/eslint-plugin-template": "^1.2.0",
    "@angular-eslint/schematics": "^1.2.0",
    "@angular-eslint/template-parser": "^1.2.0",
    "@angular/cli": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/compiler-cli": "11.2.0",
    "@angular/language-service": "11.2.0",
    "@types/jasmine": "^3.6.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.20.0",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "acorn": "^8.0.5",
    "electron": "^11.2.3",
    "electron-window-state": "^5.0.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.6.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslib": "^2.1.0",
    "typescript": "4.0.5"
  }
}
