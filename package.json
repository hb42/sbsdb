{
  "name": "@hb42/sbsdb",
  "displayname": "SBS-Datenbank",
  "description": "IT-Bestand und Hotline-Tool",
  "version": "5.0.0-alpha.1086",
  "copyright": "Copyright (c) 1997-2020 Hartmut Bauer",
  "author": "hb",
  "license": "MIT",
  "main": "start-electron.js",
  "scripts": {
    "gittag": "git log -n 1 --pretty=format:\"$(git describe --tags --long) (%cd)\" --date=format:'%d.%m.%Y %H:%M:%S' > ./dist/sbsdb/resource/git.txt",
    "devel:osx": "ng build --configuration=devx && npm run gittag",
    "devel:win": "ng build --configuration=dev && npm run gittag",
    "devel": "npm run devel:$npm_package_config_platform",
    "prod:osx": "ng build --configuration=prodx && npm run gittag",
    "prod:win": "ng build --configuration=prod && npm run gittag",
    "prod": "npm run prod:$npm_package_config_platform",
    "electron:prep": "cd dist && npm install electron-window-state",
    "electron:pack:osx": "electron-packager dist sbsdb --arch x64 --icon div/electron-packager/sbasdb --overwrite --platform darwin --out ../electron-packager/ --no-prune",
    "electron:pack:win": "electron-packager dist sbsdb --arch x64 --icon div/electron-packager/sbsdb --overwrite --platform win32 --out ../electron-packager/ --no-prune",
    "electron:build": "npm run electron:prep && npm run electron:pack:$npm_package_config_platform",
    "electron:osx": "open ../electron-packager/sbsdb-darwin-x64/sbsdb.app --args dev",
    "electron:win": "../electron-packager/sbsdb-win32-x64/sbsdb.exe dev",
    "build:nv": "npm run prod",
    "dev": "npm --no-git-tag-version version prerelease && npm run devel",
    "build": "npm --no-git-tag-version version prerelease && npm run build:nv",
    "electron": "npm run electron:build && npm run electron:$npm_package_config_platform",
    "start": "npm run build && npm run electron",
    "release:pre": "npm version prerelease && npm run build:nv",
    "release:patch": "npm version patch && npm run build:nv",
    "release:minor": "npm version minor && npm run build:nv",
    "release:major": "npm version major && npm run build:nv",
    "release": "npm run release:patch",
    "__rem": [
      "prod/dev  = make ./dist",
      "build     = make ./dist + prerelease + electron-packager ",
      "electron  = run electron",
      "start     = build + electron",
      "release   = set patch version & build prod"
    ]
  },
  "repository": "hb42/sbsdb",
  "config": {
    "platform": "win"
  },
  "dependencies": {
    "@angular/animations": "11.0.5",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "11.0.5",
    "@angular/compiler": "11.0.5",
    "@angular/core": "11.0.5",
    "@angular/forms": "11.0.5",
    "@angular/localize": "^11.0.5",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "11.0.5",
    "@angular/platform-browser-dynamic": "11.0.5",
    "@angular/router": "11.0.5",
    "@hb42/lib-client": "^1.1.20",
    "@hb42/lib-common": "^1.0.5",
    "classlist.js": "^1.1.20150312",
    "material-design-icons-iconfont": "^6.0.0",
    "popper.js": "^1.16.1",
    "rxjs": "^6.6.3",
    "semver": "^7.3.4",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1001.7",
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular-eslint/builder": "^0.8.0-beta.6",
    "@angular-eslint/eslint-plugin": "^0.8.0-beta.6",
    "@angular-eslint/eslint-plugin-template": "^0.8.0-beta.6",
    "@angular-eslint/schematics": "^0.8.0-beta.6",
    "@angular-eslint/template-parser": "^0.8.0-beta.6",
    "@angular/cli": "^11.0.5",
    "@angular/compiler-cli": "11.0.5",
    "@angular/language-service": "11.0.5",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.19.9",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "electron": "^11.1.0",
    "electron-window-state": "^5.0.3",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "postcss": "^8.2.1",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslib": "^2.0.0",
    "typescript": "4.0.5"
  }
}
